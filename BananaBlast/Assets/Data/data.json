{
  "dialogueList": [
    {
      "dialogueId": "100101",
      "nextDialogueId": "100102",
      "dialogueSetId": "1001",
      "currentSpeaker": "TutorialNpc",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "Welcome To Banana Blast, where the best wizards come to prove themselves!"
    },
    {
      "dialogueId": "100102",
      "nextDialogueId": "100103",
      "dialogueSetId": "1001",
      "currentSpeaker": "TutorialNpc",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "Your goal is to fight endless waves of monsters to be the very best!"
    },
    {
      "dialogueId": "100103",
      "nextDialogueId": "100104",
      "dialogueSetId": "1001",
      "currentSpeaker": "Player",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "Definitely! Fight monsters, level up, get stronger. I got this!"
    },
    {
      "dialogueId": "100104",
      "nextDialogueId": "100106",
      "dialogueSetId": "1001",
      "currentSpeaker": "TutorialNpc",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "Use 'W,A,S,D' to move, and 'E' to interact."
    },
    {
      "dialogueId": "100105",
      "nextDialogueId": "100112",
      "dialogueSetId": "1001",
      "currentSpeaker": "Player",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "Okayy... but how do I shoot stuff? I wanna shoot stuff!!"
    },
    {
      "dialogueId": "100106",
      "nextDialogueId": "100107",
      "dialogueSetId": "1001",
      "currentSpeaker": "TutorialNpc",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "Use your mouse cursor to aim, and left click to shoot at enemies!"
    },
    {
      "dialogueId": "100107",
      "nextDialogueId": "100108",
      "dialogueSetId": "1001",
      "currentSpeaker": "TutorialNpc",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "You can also use the scroll wheel to switch between the weapons you have on you."
    },
    {
      "dialogueId": "100108",
      "nextDialogueId": "100109",
      "dialogueSetId": "1001",
      "currentSpeaker": "Player",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "I see a shop ahead, is that how I unlock new weapons?"
    },
    {
      "dialogueId": "100109",
      "nextDialogueId": "100110",
      "dialogueSetId": "1001",
      "currentSpeaker": "TutorialNpc",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "Yup, new weapons are available as you level up, to be purchased with gold."
    },
    {
      "dialogueId": "100110",
      "nextDialogueId": "100111",
      "dialogueSetId": "1001",
      "currentSpeaker": "TutorialNpc",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "Good luck, remember you can always back out of dungeons if it gets too rough!"
    },
    {
      "dialogueId": "100111",
      "nextDialogueId": "100112",
      "dialogueSetId": "1001",
      "currentSpeaker": "Player",
      "leftImage": "PlayerSprite",
      "rightImage": "TutorialNpcSprite",
      "dialogueText": "Thank you, I'm sure to have a bananablastin' time out there!!"
    },
    {
      "dialogueId": "100112",
      "nextDialogueId": "100202",
      "dialogueSetId": "1002",
      "currentSpeaker": "Shopkeeper",
      "leftImage": "PlayerSprite",
      "rightImage": "ShopkeeperSprite",
      "dialogueText": "Hello! Welcome to my magic shop!"
    },
    {
      "dialogueId": "100113",
